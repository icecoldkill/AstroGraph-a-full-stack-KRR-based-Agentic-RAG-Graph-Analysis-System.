<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://krr.org/space_exploration.owl"
         xmlns="http://krr.org/space_exploration.owl#">

<owl:Ontology rdf:about="http://krr.org/space_exploration.owl"/>

<owl:ObjectProperty rdf:about="#launchedBy">
  <rdfs:domain rdf:resource="#SpaceMission"/>
  <rdfs:range rdf:resource="#Agency"/>
  <owl:inverseOf rdf:resource="#inverseLaunchedBy"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#launchedFrom">
  <rdfs:domain rdf:resource="#SpaceMission"/>
  <rdfs:range rdf:resource="#LaunchSite"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#carriedPayload">
  <rdfs:domain rdf:resource="#SpaceMission"/>
  <rdfs:range rdf:resource="#Payload"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasOrbit">
  <rdfs:domain rdf:resource="#SpaceMission"/>
  <rdfs:range rdf:resource="#Orbit"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isSuccess">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#SpaceMission"/>
  <rdfs:range rdf:resource="#LaunchStatus"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasAgency">
  <rdfs:domain rdf:resource="#Rocket"/>
  <rdfs:range rdf:resource="#Agency"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#refersToCountry">
  <rdfs:domain rdf:resource="#Agency"/>
  <rdfs:range rdf:resource="#Country"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#inverseLaunchedBy">
  <owl:inverseOf rdf:resource="#launchedBy"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasUniqueMissionID">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
  <rdfs:domain rdf:resource="#SpaceMission"/>
  <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#hasBudget">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#SpaceMission"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#launchDate">
  <rdfs:domain rdf:resource="#SpaceMission"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#missionCost">
  <rdfs:domain rdf:resource="#SpaceMission"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#rocketStatus">
  <rdfs:domain rdf:resource="#Rocket"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#payloadWeight">
  <rdfs:domain rdf:resource="#Payload"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#environmentalImpact">
  <rdfs:domain rdf:resource="#SpaceMission"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#missionName">
  <rdfs:domain rdf:resource="#SpaceMission"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Entity">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Agency">
  <rdfs:subClassOf rdf:resource="#Entity"/>
</owl:Class>

<owl:Class rdf:about="#GovernmentAgency">
  <rdfs:subClassOf rdf:resource="#Agency"/>
</owl:Class>

<owl:Class rdf:about="#PrivateCompany">
  <rdfs:subClassOf rdf:resource="#Agency"/>
</owl:Class>

<owl:Class rdf:about="#SpaceMission">
  <rdfs:subClassOf rdf:resource="#Entity"/>
</owl:Class>

<owl:Class rdf:about="#CrewedMission">
  <rdfs:subClassOf rdf:resource="#SpaceMission"/>
</owl:Class>

<owl:Class rdf:about="#UncrewedMission">
  <rdfs:subClassOf rdf:resource="#SpaceMission"/>
</owl:Class>

<owl:Class rdf:about="#MoonMission">
  <rdfs:subClassOf rdf:resource="#SpaceMission"/>
</owl:Class>

<owl:Class rdf:about="#MarsMission">
  <rdfs:subClassOf rdf:resource="#SpaceMission"/>
</owl:Class>

<owl:Class rdf:about="#InterstellarMission">
  <rdfs:subClassOf rdf:resource="#SpaceMission"/>
</owl:Class>

<owl:Class rdf:about="#Rocket">
  <rdfs:subClassOf rdf:resource="#Entity"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasAgency"/>
      <owl:onClass rdf:resource="#Agency"/>
      <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#ReusableRocket">
  <rdfs:subClassOf rdf:resource="#Rocket"/>
</owl:Class>

<owl:Class rdf:about="#ExpendableRocket">
  <rdfs:subClassOf rdf:resource="#Rocket"/>
</owl:Class>

<owl:Class rdf:about="#Satellite">
  <rdfs:subClassOf rdf:resource="#Entity"/>
</owl:Class>

<owl:Class rdf:about="#CommunicationSatellite">
  <rdfs:subClassOf rdf:resource="#Satellite"/>
</owl:Class>

<owl:Class rdf:about="#WeatherSatellite">
  <rdfs:subClassOf rdf:resource="#Satellite"/>
</owl:Class>

<owl:Class rdf:about="#SpySatellite">
  <rdfs:subClassOf rdf:resource="#Satellite"/>
</owl:Class>

<owl:Class rdf:about="#Payload">
  <rdfs:subClassOf rdf:resource="#Entity"/>
</owl:Class>

<owl:Class rdf:about="#ScientificInstrument">
  <rdfs:subClassOf rdf:resource="#Payload"/>
</owl:Class>

<owl:Class rdf:about="#LaunchSite">
  <rdfs:subClassOf rdf:resource="#Entity"/>
</owl:Class>

<owl:Class rdf:about="#SpacePort">
  <rdfs:subClassOf rdf:resource="#LaunchSite"/>
</owl:Class>

<owl:Class rdf:about="#Person">
  <rdfs:subClassOf rdf:resource="#Entity"/>
</owl:Class>

<owl:Class rdf:about="#Astronaut">
  <rdfs:subClassOf rdf:resource="#Person"/>
</owl:Class>

<owl:Class rdf:about="#SpaceTourist">
  <rdfs:subClassOf rdf:resource="#Person"/>
</owl:Class>

<owl:Class rdf:about="#Orbit">
  <rdfs:subClassOf rdf:resource="#Entity"/>
</owl:Class>

<owl:Class rdf:about="#LowEarthOrbit">
  <rdfs:subClassOf rdf:resource="#Orbit"/>
</owl:Class>

<owl:Class rdf:about="#GeostationaryOrbit">
  <rdfs:subClassOf rdf:resource="#Orbit"/>
</owl:Class>

<owl:Class rdf:about="#CelestialBody">
  <rdfs:subClassOf rdf:resource="#Entity"/>
</owl:Class>

<owl:Class rdf:about="#Planet">
  <rdfs:subClassOf rdf:resource="#CelestialBody"/>
</owl:Class>

<owl:Class rdf:about="#Moon">
  <rdfs:subClassOf rdf:resource="#CelestialBody"/>
</owl:Class>

<owl:Class rdf:about="#Star">
  <rdfs:subClassOf rdf:resource="#CelestialBody"/>
</owl:Class>

<owl:Class rdf:about="#Country">
  <rdfs:subClassOf rdf:resource="#Entity"/>
</owl:Class>

<owl:Class rdf:about="#Continent">
  <rdfs:subClassOf rdf:resource="#Entity"/>
</owl:Class>

<owl:Class rdf:about="#MissionSuccessStatus">
  <rdfs:subClassOf rdf:resource="#Entity"/>
</owl:Class>

<owl:Class rdf:about="#LaunchStatus">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:oneOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Success"/>
        <rdf:Description rdf:about="#Failure"/>
        <rdf:Description rdf:about="#PartialFailure"/>
        <rdf:Description rdf:about="#Scheduled"/>
      </owl:oneOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#HighBudgetMission">
  <rdfs:subClassOf rdf:resource="#SpaceMission"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#SpaceMission"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#hasBudget"/>
          <owl:someValuesFrom>
            <rdfs:Datatype>
              <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
              <owl:withRestrictions>
                <rdf:Description>
                  <rdf:first>
                    <rdf:Description>
                      <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1000.0</xsd:minInclusive>
                    </rdf:Description>
                  </rdf:first>
                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                </rdf:Description>
              </owl:withRestrictions>
            </rdfs:Datatype>
          </owl:someValuesFrom>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#LaunchEntity">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#GovernmentAgency"/>
        <rdf:Description rdf:about="#PrivateCompany"/>
      </owl:unionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#SuccessfulMoonMission">
  <rdfs:subClassOf rdf:resource="#SpaceMission"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#MoonMission"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#isSuccess"/>
          <owl:hasValue rdf:resource="#Success"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#FailedMission">
  <rdfs:subClassOf rdf:resource="#SpaceMission"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#SpaceMission"/>
        <owl:Class>
          <owl:complementOf>
            <owl:Restriction>
              <owl:onProperty rdf:resource="#isSuccess"/>
              <owl:hasValue rdf:resource="#Success"/>
            </owl:Restriction>
          </owl:complementOf>
        </owl:Class>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#CommercialSpaceFlight">
  <rdfs:subClassOf rdf:resource="#SpaceMission"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#SpaceMission"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#launchedBy"/>
          <owl:someValuesFrom rdf:resource="#PrivateCompany"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#NonGovernmentMission">
  <rdfs:subClassOf rdf:resource="#SpaceMission"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#SpaceMission"/>
        <owl:Class>
          <owl:complementOf>
            <owl:Restriction>
              <owl:onProperty rdf:resource="#launchedBy"/>
              <owl:someValuesFrom rdf:resource="#GovernmentAgency"/>
            </owl:Restriction>
          </owl:complementOf>
        </owl:Class>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:NamedIndividual rdf:about="#Success">
  <rdf:type rdf:resource="#LaunchStatus"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Failure">
  <rdf:type rdf:resource="#LaunchStatus"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#PartialFailure">
  <rdf:type rdf:resource="#LaunchStatus"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Scheduled">
  <rdf:type rdf:resource="#LaunchStatus"/>
</owl:NamedIndividual>


</rdf:RDF>
