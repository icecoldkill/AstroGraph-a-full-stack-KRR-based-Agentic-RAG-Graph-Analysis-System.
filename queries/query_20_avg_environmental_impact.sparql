PREFIX onto: <http://krr.org/space_exploration.owl#>
PREFIX ex: <http://krr.org/space/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT ?agency (AVG(?impact) AS ?avgImpact) (COUNT(?mission) AS ?missionCount)
WHERE {
    ?mission rdf:type onto:SpaceMission .
    ?mission onto:launchedBy ?agency .
    ?mission onto:environmentalImpact ?impact .
}
GROUP BY ?agency
ORDER BY DESC(?avgImpact)

