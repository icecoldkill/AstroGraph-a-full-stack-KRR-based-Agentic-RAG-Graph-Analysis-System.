PREFIX onto: <http://krr.org/space_exploration.owl#>
PREFIX ex: <http://krr.org/space/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT ?mission ?missionName ?payload ?weight
WHERE {
    ?mission rdf:type onto:SpaceMission .
    ?mission onto:missionName ?missionName .
    ?mission onto:carriedPayload ?payload .
    ?payload onto:payloadWeight ?weight .
}
ORDER BY DESC(?weight)

